   <div class="col-md-12">
        {{#if savedArticles}}
            {{#each savedArticles}}
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4>{{this.title}}</h4>
                </div>
                <div class="panel-body">
                    <p> {{this.summary}}</p>
                    <a href={{this.link}} target="_blank">{{this.link}}</a>
                    <br><br>
                    <!-- Button -->
                    <button type="button" class="btn btn-default note" data-id={{this._id}}>Save Note</button>
                </div>
            </div>
            {{/each}}
        {{else}}
        <div class="panel panel-default">
            <div class="panel-body">
            <h4>Placeholder</h4>
            </div>
         </div>
        {{/if}}
    </div>